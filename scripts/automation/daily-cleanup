#!/data/data/com.termux/files/usr/bin/bash
termux-notification -t "Daily Cleanup" -c "Starting..."

# Clean Termux cache
pkg clean
apt autoclean

# Clean old logs (>7 days)
find ~/ai/outputs/ -name "*.log" -mtime +7 -delete 2>/dev/null

# Clean old benchmarks (keep last 30 days)
find ~/ai/outputs/benchmarks/ -mtime +30 -delete 2>/dev/null

# Sync to GitHub
cd ~/termux-ai-workspace && git add . && git commit -m "Auto: daily checkpoint $(date 

Co-authored-by: Qwen-Coder <qwen-coder@alibabacloud.com>"+%Y-%m-%d")" && git push origin main 2>/dev/null

termux-notification -t "Cleanup Complete" -c "System cleaned"
